    "graphql-tag": "2.5.0",
    "gzip-size": "4.1.0",
    "html-webpack-plugin": "3.0.6",
    "mini-css-extract-plugin": "0.2.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "postcss-nested": "3.0.0",
    "postcss-nesting": "4.2.1",
    "postcss-remify": "1.0.1",
    "postcss-scss": "1.0.3",
