    "@semantic-release/git": "5.0.0",
    "@vital-software/babel-preset-vital-software": "^1.0.0",
    "react": "16.2.0",
    "semantic-release": "15.6.0"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
