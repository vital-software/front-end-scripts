{
  "name": "vitalizer",
  "version": "3.4.3",
  "description": "The Vital Software front-end asset builder.",
  "license": "MIT",
  "repository": "vital-software/vitalizer",
  "homepage": "https://github.com/vital-software/vitalizer#readme",
  "author": "Te Riu Warren <teriu@teriu.name>",
  "bugs": {
    "url": "https://github.com/vital-software/vitalizer/issues"
  },
  "engines": {
    "node": ">=8.1.3"
  },
  "files": [
    ".babelrc",
    ".eslintrc.json",
    ".stylelintrc.json",
    "postcss.config.js",
    "webpack.config.babel.js",
    "bin",
    "helper",
    "script"
  ],
  "release": {
    "verifyConditions": [
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "precommit": "yarn test-lint",
    "test": "./test/task/local-test.sh",
    "test-lint": "yarn test-lint-css && yarn test-lint-js",
    "test-lint-css": "stylelint \"test/app/sass/**/*.{css,scss}\" --syntax scss",
    "test-lint-js": "eslint \"test/app/**/*.{js,jsx}\" --quiet"
  },
  "bin": {
    "vitalizer": "bin/vitalizer.js"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babili-webpack-plugin": "0.1.1",
    "brotli-webpack-plugin": "0.4.1",
    "chalk": "2.3.0",
    "cross-spawn": "5.1.0",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "detect-port": "1.2.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "filesize": "3.5.11",
    "fs-extra": "4.0.2",
    "graphql": "0.11.7",
    "graphql-tag": "2.5.0",
    "gzip-size": "4.1.0",
    "html-webpack-plugin": "2.30.1",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "postcss-nested": "3.0.0",
    "postcss-nesting": "4.2.1",
    "postcss-remify": "1.0.1",
    "postcss-scss": "1.0.3",
    "postcss-simple-vars": "4.1.0",
    "postcss-strip-inline-comments": "0.1.5",
    "react-hot-loader": "3.1.3",
    "recursive-readdir": "2.2.1",
    "strip-ansi": "4.0.0",
    "style-loader": "github:webpack-contrib/style-loader#master",
    "stylelint-order": "0.6.0",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  },
  "devDependencies": {
    "@semantic-release/git": "4.0.0",
    "babel-eslint": "8.2.2",
    "eslint": "4.18.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-cypress": "2.0.1",
    "eslint-plugin-filenames": "1.2.0",
    "eslint-plugin-flowtype": "2.45.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.12.2",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "greenkeeper-lockfile": "https://github.com/vital-software/greenkeeper-lockfile#2fcba1e7cd0e5975ad2cb572e7beb0c8b740c5f6",
    "husky": "0.14.3",
    "semantic-release": "15.0.0"
  }
}
