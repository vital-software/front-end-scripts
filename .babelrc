{
  "plugins": [
    "transform-class-properties",
    "transform-object-assign",
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "Chrome >= 59",
          "Firefox >= 55",
          "Safari >= 10.3",
          "android 4.4.3",
          "ios 10"
        ]
      },
      "modules": false,
      "useBuiltIns": true,
      "debug": true
    }],
    "react",
    "flow"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
