{
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "presets": [
    ["env", {
      "modules": false,
      "loose": true,
      "useBuiltIns": "entry",
      "debug": false
    }],
    "react",
    "flow"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
